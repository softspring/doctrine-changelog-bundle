{
    "name": "softspring/doctrine-changelog-bundle",
    "description": "Doctrine changelog collect and store into warehouse",
    "type": "symfony-bundle",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "doctrine/orm": "^3.1",
        "symfony/config": "^5.4|^6.0|^7.0",
        "symfony/dependency-injection": "^5.4|^6.0|^7.0",
        "symfony/http-foundation": "^5.4|^6.0|^7.0",
        "symfony/http-kernel": "^5.4|^6.0|^7.0",
        "symfony/security-core": "^5.4|^6.0|^7.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "3.51.*",
        "google/cloud-bigquery": "^1.0",
        "phpstan/phpstan": "^1.10"
    },
    "suggest": {
        "google/cloud-bigquery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Softspring\\DoctrineChangeLogBundle\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
